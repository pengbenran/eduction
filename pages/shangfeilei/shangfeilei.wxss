.head{
height:160rpx;
background: #fff;
display: flex;
padding: 20rpx;
border-top:1px solid #eee;
border-bottom:1px solid #eee;
}
.portrait-left{
  width: 20%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.port-img{
  width: 130rpx;
 height: 130rpx;
 border-radius: 50%;
}
.port-img image{
 width: 100%;
 height: 100%;
 border-radius: 50%;
}
.rait-bottom{
  display: flex;
  align-items: center;
}
.port-left{
  width: 70rpx;
  height: 40rpx;
  background: #fb4d44;
  color: #fff;
  border-radius: 8rpx;
  text-align: center;
  line-height: 38rpx;
  font-size: 26rpx;
}
.rait-right image{
 width: 20rpx;
 height: 32rpx;
}
.portrait-right{
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 20rpx;
}
.rait-left{
  width: 88%;
  height: 90%;
  display: flex;
  flex-direction: column;
 justify-content: space-around;
 font-size: 32rpx;
}
.port-text{
  font-size: 28rpx;
  color: #ff4b46;
}

/* 公告 */
.head-bottom{
  display: flex;
  height: 80rpx;
  padding-left:20rpx;
  background: #f5e6c9;
  align-items: center;
}
.head-box{
 width:70rpx;
 height: 46rpx;
 background:#ccc;
 text-align: center;
 font-size: 26rpx;
 color: #fff;
 border-radius: 6rpx;
 line-height: 46rpx;
}
.head-text{
  font-size: 28rpx;
  margin-left: 20rpx;
  color: #515151;
}
/* 导航 */
.box{
    position: relative;
    width:100%;
    color: #555;
    height: 1000rpx;
}
.left_list{
    width:25%;
    background: #fff;
}
.list_item{
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    width:100%;
    height:45px;
    line-height: 45px;
    border-bottom:1px solid #ccc;
    color: #000;
    text-align: center;
    font-size:14px;
}
.active{
    color:#27b4fb; 
}
.right_cont{
    width:75%;
    right:0;
    top:0;
    position: absolute;
}
.right_item{
    width:33.33%;
    height:80px;
    float: left;
}
.img{
    width:50px;
    height:30px;
    display: block;
    margin:0 auto;
}
.text{
    font-size: 12px;
    text-align: center;
    padding-top:5px;
    overflow: hidden;
    white-space: nowrap;
}
.mes{
    color:#515151;
    background:#fff;
    margin-left: 1rpx;
    height:70rpx;
    font-size: 13px;
    line-height: 70rpx;
    padding-left: 20rpx;
}

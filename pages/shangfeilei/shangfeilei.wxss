/*下拉*/
.choice{
background: #fff;
display: flex;
padding: 10rpx 20rpx 10rpx 10rpx;
border: 1rpx solid #eee;
}
.cho-left{
width: 200rpx;
display: flex;
justify-content: center;
align-items: center;
height: 200rpx;
}
.cho-left image{
width: 100%;
height:100%;
display: block;
}
.noun-right{
display: flex;
flex-direction: column;
flex-grow: 1;
justify-content: space-around;
}
.noun-top{
  display: flex;
  justify-content: space-between;
}
.jiahao image{
  width: 50rpx;
  height: 50rpx;
}
